                <tbody>
                  <tr class="discussion">
                    <td class="hidden-xs">
                      <a href="#" title="Satheesh kumar">
                        <%= image_tag('no-avatar.png', height: '32', width: '32', class: 'img-circle') %>
                      </a>
                    </td>
                    <td>
                      <div class="discussion-content">
                        <a href="#">Login details not working - </a><span>Hi Satheesh, I have completed the user login functionality, and exporting</span>
                      </div>
                      <div class="attachments nav-group-hz">
                        <a href="#"><%= image_tag('no-avatar.png', height: '24') %></a>
                        <a href="#"><%= image_tag('no-avatar.png', height: '24') %></a>
                        <a href="#"><%= image_tag('no-avatar.png', height: '24') %></a>
                      </div>
                    </td>
                    <td>
                      <small>
                        <span>Jan 25 2016</span>
                        <span>03:30pm</span>
                      </small>
                    </td>
                  </tr>
                  <tr class="discussion">
                    <td class="hidden-xs">
                      <a href="#" title="Satheesh kumar">
                        <%= image_tag('no-avatar.png', height: '32', width: '32', class: 'img-circle') %>
                      </a>
                    </td>
                    <td width="70%">
                      <div class="discussion-content">
                        <a href="#">Login details not working - </a><span>Hi Satheesh, I have completed the user login functionality, and exporting</span>
                      </div>
                    </td>
                    <td>
                      <small>
                        <span>Jan 25 2016</span>
                        <span>03:30pm</span>
                      </small>
                    </td>
                  </tr>
                  <tr class="discussion">
                    <td class="hidden-xs">
                      <a href="#" title="Satheesh kumar">
                        <%= image_tag('no-avatar.png', height: '32', width: '32', class: 'img-circle') %>
                      </a>
                    </td>
                    <td width="70%">
                      <div class="discussion-content">
                        <a href="#">Login details not working - </a><span>Hi Satheesh, I have completed the user login functionality, and exporting</span>
                      </div>
                      <div class="attachments nav-group-hz">
                        <a href="#"><%= image_tag('no-avatar.png', height: '24') %></a>
                        <a href="#"><%= image_tag('no-avatar.png', height: '24') %></a>
                        <a href="#"><%= image_tag('no-avatar.png', height: '24') %></a>
                      </div>
                    </td>
                    <td>
                      <small>
                        <span>Jan 25 2016</span>
                        <span>03:30pm</span>
                      </small>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
          <div class="panel attachments-panel" id="attachmentsPanel">
            <div class="panel-heading">
              <h3 class="panel-title pull-left"><i class="fa fa-files-o"></i> Attachments</h3>
              <a href="#" class="pull-right">See all files</a>
              <div class="clearfix"></div>
            </div>
            <div class="panel-body">
              <div class="attachments">
                <div class="row">
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="17 MB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="13 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="23 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                  <div class="col-sm-3 col-xs-6">
                    <div class="attachment thumbnail"><a href="#attachmentsPanel" class="lightbox" data-show-modal="true" data-file-size="423 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '100%') %></a></div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
      </div>
    </section>
  </article>
  <div class="modal fade" id="attachmentLightBoxModal" tabindex="-1" role="dialog" aria-labelledby="attachmentLightBoxModalTitle" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title lightbox-file-title" id="attachmentLightBoxModalTitle">Attachment Modal</h4>
        </div>
        <div class="modal-body">
          <div class="row">
            <div class="col-sm-4">
              <div class="lightbox-img"></div>
            </div>
            <div class="col-sm-8">
              <div class="lightbox-content">
                <h3 class="lightbox-file-title"></h3>
                <p>
                  <label>Size: </label><small class="lightbox-file-size"></small>
                </p>
              </div>
            </div>
          </div>
          <div class="lightbox-thumbnails">
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="12 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="523 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="23 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="13 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
            <div class="thumbnail attachment"><a href="#attachmentsPanel" class="lightbox" data-file-size="123 KB" data-file-name="no-avatar.png"><%= image_tag('no-avatar', width: '32') %></a></div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary"><i class="fa fa-download"></i> Download</button>
        </div>
      </div>
    </div>
  </div>
  <div class="modal fade" id="inviteUserModal" tabindex="-1" role="dialog" aria-labelledby="inviteUserModalTitle" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" id="inviteUserModalTitle">Invite User</h4>
        </div>
        <div class="modal-body">
          <%= form_tag(class: 'form form-horizontal') do |f| %>
            <div class="form-group">
              <label for="">Email address:</label>
              <%= text_field_tag :email, nil, class: 'form-control' %>
            </div>
          <% end %>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary">Send Mail</button>
        </div>
      </div>
    </div>
  </div>
</section>
